<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>net8.0-android</TargetFramework>
		<SupportedOSPlatformVersion>26</SupportedOSPlatformVersion>
		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>

		<PackageId>IDotta.Android.UsbSerial</PackageId>
		<Version>0.0.2</Version>
		<Title>UsbSerial4Android</Title>
		<Product>UsbSerial4Android</Product>
		<Authors>IÃºri Dotta</Authors>
		<Description>A .NET library for serial usb in Android</Description>
		<PackageProjectUrl>https://github.com/idotta/android-serial-dotnet</PackageProjectUrl>
		<PackageTags>android;serial;usb;csharp</PackageTags>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<RepositoryUrl>https://github.com/idotta/android-serial-dotnet</RepositoryUrl>
		<Copyright>MIT</Copyright>
		<Company>Dotta IT Solutions</Company>
		<!--<PackageIcon></PackageIcon>-->
